    "@nuxt/ui-pro": "^3.3.2",
    "@nuxtjs/i18n": "^10.0.3",
    "@nuxtjs/seo": "^3.1.0",
    "@vueuse/nuxt": "^13.6.0",
    "@nuxt/fonts": "^0.11.4",
