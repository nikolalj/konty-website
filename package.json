{
  "name": "konty-website",
  "private": true,
  "type": "module",
  "version": "v0.0.1",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "preview": "pnpm run build && wrangler dev",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "typecheck": "nuxt typecheck",
